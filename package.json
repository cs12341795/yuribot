{
  "name": "yuribot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:tsc",
    "build:tsc": "tsc",
    "watch": "npm run build:tsc -- -w",
    "lint": "npm run lint:tslint",
    "lint:tslint": "tslint --project . {server}/**/*.ts",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "dev": "cross-env NODE_ENV=development ts-node server/index.ts",
    "test": "jest --runInBand",
    "clean": "rm -rf build public/build",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cs12341795/yuribot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cs12341795/yuribot/issues"
  },
  "homepage": "https://github.com/cs12341795/yuribot#readme",
  "devDependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/jest": "^22.2.3",
    "@types/koa": "^2.0.46",
    "@types/supertest": "^2.0.5",
    "jest": "^22.4.4",
    "supertest": "^3.1.0",
    "ts-jest": "22.4.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "cross-env": "^5.2.0",
    "discord.js": "^11.3.2",
    "koa": "^2.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
